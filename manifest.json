{
  "manifest_version": 3,
  "name": "Mobile Link Receiver",
  "version": "1.0",
  "description": "Receives links from mobile and opens in Chrome",
  "permissions": ["storage", "tabs", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
